<h1>Listing job_posters</h1>

<table>
  <thead>
    <tr>
      <th>Headline</th>
      <th>Details</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @job_posters.each do |job_poster| %>
      <tr>
        <td><%= job_poster.Headline %></td>
        <td><%= job_poster.Details %></td>
        <td><%= link_to 'Show', job_poster %></td>
        <td><%= link_to 'Edit', edit_job_poster_path(job_poster) %></td>
        <td><%= link_to 'Destroy', job_poster, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Job poster', new_job_poster_path %>
